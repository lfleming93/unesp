<p id="notice"><%= notice %></p>

<h1>Listing Installations</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Area</th>
      <th>Volume</th>
      <th>Localizacao</th>
      <th>Tipo</th>
      <th>Sponsor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @installations.each do |installation| %>
      <tr>
        <td><%= installation.descricao %></td>
        <td><%= installation.area %></td>
        <td><%= installation.volume %></td>
        <td><%= installation.localizacao %></td>
        <td><%= installation.tipo %></td>
        <td><%= installation.sponsor %></td>
        <td><%= link_to 'Show', installation %></td>
        <td><%= link_to 'Edit', edit_installation_path(installation) %></td>
        <td><%= link_to 'Destroy', installation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Installation', new_installation_path %>
